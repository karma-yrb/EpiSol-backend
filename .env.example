# Example environment variables for Render deployment
# Copiez ce fichier en .env et adaptez les valeurs pour la production

# Port d'écoute du serveur (Render définit automatiquement PORT)
PORT=10000

# Clé secrète JWT
JWT_SECRET=change_this_secret

# Configuration MySQL
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=yourpassword
DB_NAME=episol
DB_PORT=3306

# CORS autorisé (frontend)
CORS_ORIGIN=https://episol.yade-services.fr
